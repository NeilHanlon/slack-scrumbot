{
  "name": "slack-scrumbot",
  "version": "0.1.0",
  "description": "A Slack bot that performs a pre-standup checkin.",
  "config": {
    "waitMinutes": 2
  },
  "main": "index.js",
  "scripts": {
    "start": "forever start -c babel-node index.js",
    "stop": "forever stop index.js",
    "watch": "nodemon --exec babel-node -- index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smhg/slack-scrumbot.git"
  },
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "debug": "^2.2.0",
    "forever": "0.15.1",
    "moment": "^2.10.6",
    "slack-client": "1.5.0"
  },
  "devDependencies": {
    "nodemon": "1.8.1",
    "babel-eslint": "4.1.6",
    "eslint": "1.10.3"
  }
}
